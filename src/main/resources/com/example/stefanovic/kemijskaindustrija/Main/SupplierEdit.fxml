<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="503.0" prefWidth="600.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stefanovic.kemijskaindustrija.Controllers.Supplier.SupplierEdit">
    <fx:include source="NavBar.fxml" />

    <Button layoutX="450.0" layoutY="379.0" mnemonicParsing="false" onAction="#saveSupplier" prefHeight="25.0" prefWidth="245.0" text="Save Changes" />
    <AnchorPane layoutX="273.0" layoutY="92.0" prefHeight="268.0" prefWidth="562.0" style="-fx-background-color: #2A4858;">
        <children>
            <Label layoutX="9.0" layoutY="39.0" text="Name" underline="true" />
            <Label layoutX="9.0" layoutY="95.0" text="Street" underline="true" />
            <Label layoutX="9.0" layoutY="145.0" text="Street Number" underline="true" />
            <TextField fx:id="supplierNameTextField" layoutX="111.0" layoutY="35.0" prefHeight="25.0" prefWidth="409.0" />
            <TextField fx:id="supplierStreetNameTextField" layoutX="114.0" layoutY="91.0" prefHeight="25.0" prefWidth="409.0" />
            <TextField fx:id="supplierStreetNumberTextField" layoutX="114.0" layoutY="141.0" prefHeight="25.0" prefWidth="409.0" />
            <Label fx:id="nameErrorLabel" layoutX="112.0" layoutY="62.0" styleClass="error" text="nameErrorLabel" textFill="RED" underline="true" />
            <Label fx:id="streetErrorLabel" layoutX="114.0" layoutY="118.0" styleClass="error" text="streetErrorLabel" textFill="RED" underline="true" />
            <Label fx:id="streetNumberErrorLabel" layoutX="114.0" layoutY="168.0" styleClass="error" text="streetNumberErrorLabel" textFill="RED" underline="true" />
            <Label layoutX="9.0" layoutY="210.0" text="City" underline="true" />
            <TextField fx:id="supplierCityTextField1" layoutX="114.0" layoutY="206.0" prefHeight="25.0" prefWidth="409.0" />
            <Label fx:id="cityErrorLabel" layoutX="114.0" layoutY="233.0" styleClass="error" text="cityErrorLabel" textFill="RED" underline="true" />
        </children>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
        </padding>
    </AnchorPane>
   <AnchorPane layoutX="924.0" prefHeight="504.0" prefWidth="245.0" style="-fx-background-color: #2A4858;">
      <children>
         <ComboBox fx:id="chemicalsComboBox" layoutX="26.0" layoutY="80.0" prefHeight="25.0" prefWidth="193.0" promptText="Pick Chemical" />
         <Button layoutX="38.0" layoutY="132.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="169.0" text="Add" />
         <Label layoutX="26.0" layoutY="41.0" prefWidth="169.0" text="Add Chemical To Supplier">
            <font>
               <Font size="20.0" />
            </font>
         </Label>
      </children>
   </AnchorPane>
    <Label fx:id="title" layoutX="14.0" layoutY="38.0" text="Edit Supplier Information">
        <font>
            <Font size="20.0" />
        </font>
    </Label>
</AnchorPane>
